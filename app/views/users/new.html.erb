<div class="piano-img">
	<section class="row">
		<div class="container">
			<div class="col-md-4">
			  <form class"form-signin" method="post" action="<%= users_url %>" role="form">
					<h1 class="form-signin-heading login-header">Sign Up</h1>
					<h4 class="login-header">Already a member? <a href="../session/new">Log In</a></h4>
			    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
			    <input type="text" class="form-control" name="user[username]"
			        placeholder="Username" required autofocus>
					<input type="text" class="form-control" name="user[email]"
			        placeholder="Email" required autofocus>
				  <input type="password" name="user[password]"
			        class="form-control" placeholder="Password" required autofocus>
					<input type="submit" class="btn btn-lg btn-block btn-primary" value="Sign Up">
				</form>
			</div>

			<div class="col-md-1"></div>

			<div class="col-md-2">
				<img src="<%= image_path 'organ_logo.png' %>" id="splash-logo"></img>
			</div>

			<div class="col-md-5"></div>
		</div>
	</section>
</div>
